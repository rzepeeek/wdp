tab = [4,3,1,2,5]
element = 3

def czy_nalezy(tab, element):
    for e in tab:
        if e == element:
            return True

    return False

def czy_zawiera(lista1, lista2):
    for e in lista2:
        if not czy_zawiera(lista1, e):
            return False

    return True

// NAPISZ FUNCKJE remove(napis, usuwany), ktorej wynikiem jest napis powstaly poprzez z napisu
// bedacego jej pierwszym argumentem, pierwszego wystapienia napisu bedacego jej drugim argumentem
// Przykladowo wynikiem wywolania remove("abrakadabra", "ab") bedzie napis "rakadabra"
// W tej funckji main przetestuj dzialanie funckji remove.

napis = 'abrakadabra'
usuwany = 'ab'

def remove(napis, usuwany):
    index = napis.find(usuwany)
    if index != -1:
        napis = napis[:index] + napis[index + len(usuwany):]
    return napis

wynik = remove(napis, usuwany)
print(f"{wynik}")

// NAPISZ FUNCKJE remove_all(napis, usuwany) ktorej wynikiem jest napis powstaly poprzez usuniecie
// z napisu bedacego jej pierwszym argumentem, wsyzstkich wystapien napisu bedacego jej drugim argumentem
// przykladowo wynikiem wywolania remove("abrakadabra", "ab") bedzie napis "rakadra" W funkcji
// main przetestuj dzialaie funckji remove_all

napis = 'abrakadabra'
usuwany = 'ab'

def remove_all(napis, usuwany):
    while napis.find(usuwany) != -1:
        index = napis.find(usuwany)
        napis = napis[:index] + napis[index + len(usuwany):]
    return napis

wynik = remove_all(napis, usuwany)
print(f"{wynik}")



